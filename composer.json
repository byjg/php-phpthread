{
  "name": "byjg/phpthread",
  "description": "Polyfill Implementation of Threads in PHP. This class supports both FORK process and native Threads using ZTS compilation.",
  "require": {
    "byjg/cache-engine": "^6.0",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "php": ">=8.1 <8.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10|^11",
    "vimeo/psalm": "^5.9|^6.12",
    "react/promise": "^3.2",
    "react/event-loop": "^1.5"
  },
  "suggest": {
    "ext-pthreads": "*"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "ByJG\\PHPThread\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  }
}
